sudo -u postgres createuser -d sj

sudo apt install mysql-server mysql-client
sudo mysql mysql <<EOF
CREATE USER 'sj'@'localhost' IDENTIFIED BY '';
CREATE DATABASE sj;
GRANT ALL PRIVILEGES ON sj.* TO 'sj'@'localhost';
EOF

sudo apt  install tree

